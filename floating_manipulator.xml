<?xml version="1.0"?>

<!-- space dyn講習会 補足資料6 -->
<sdf version="1.9">
  <model name="floating_manipulator">
    <static>false</static>

    <!-- Link 0: 1700kg cube -->
    <link name="link0">
      <!--  ******************pose -->
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>1700</mass>
        <inertia>
          <ixx>1434</ixx>
          <iyy>1434</iyy>
          <izz>1735</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>3.5 3.5 3.5</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>3.5 3.5 3.5</size>
          </box>
        </geometry>
      </visual>
    </link>

    <!-- Link 1:5kg, 0.25m long -->
    <link name="link1">
      <!--  ******************pose -->
      <pose>0 0 1.875 0 0 0</pose>
      <inertial>
        <!--  ******************mass -->
        <mass>5.0</mass>
        <!--  ******************inertia -->
        <inertia>
          <ixx>0.0292</ixx>
          <iyy>0.0292</iyy>
          <izz>0.00625</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <!--  ******************col size -->
          <cylinder>
            <radius>0.2</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <!--  ******************vis size-->
          <cylinder>
            <radius>0.2</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </visual>
    </link>

    <!-- Link 1: 50, 2.5m long -->
    <link name="link2">
      <!--
      SDFの<pose>はワールド座標系での位置姿勢を指定します。link1からの相対姿勢を直接指定することはできませんが、<joint>でlink2をlink1に接続し、<pose>を使わずに相対位置は<joint>の<pose>で指定します。link2の<pose>は省略または0にしてください。-->
      <!--  ******************pose -->
      <pose>0 0 3.25 0 0 0</pose>
      <inertial>
        <!--  ******************mass -->
        <mass>50.0</mass>
        <!--  ******************inertia -->
        <inertia>
          <ixx>26.1</ixx>
          <iyy>26.1</iyy>
          <izz>0.0625</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <!--  ******************col size -->
          <cylinder>
            <radius>0.2</radius>
            <length>2.5</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <!--  ******************vis size-->
          <cylinder>
            <radius>0.2</radius>
            <length>2.5</length>
          </cylinder>
        </geometry>
      </visual>
    </link>

    <!-- Link 1: 50, 2.5m long -->
    <link name="link3">
      <!--
      SDFの<pose>はワールド座標系での位置姿勢を指定します。link1からの相対姿勢を直接指定することはできませんが、<joint>でlink2をlink1に接続し、<pose>を使わずに相対位置は<joint>の<pose>で指定します。link2の<pose>は省略または0にしてください。-->
