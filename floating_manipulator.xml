    <!-- Joint 0: link0 - link1 -->
    <joint name="joint0" type="revolute">
      <parent>link0</parent>
      <child>link1</child>
      <pose>0 0 -0.125 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>

      <initial_position>2</initial_position>
    </joint>

    <!-- Joint 1: link1 - link2 -->
    <joint name="joint1" type="revolute">
      <parent>link1</parent>
      <child>link2</child>
      <pose>0 0 -1.25 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <!-- Joint 2: link2 - link3 -->
    <joint name="joint2" type="revolute">
      <parent>link2</parent>
      <child>link3</child>
      <pose>0 0 -1.25 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <!-- Joint 3: link3 - link4 -->
    <joint name="joint3" type="revolute">
      <parent>link3</parent>
      <child>link4</child>
      <pose>0 0 -0.25 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <!-- Joint 4: link4 - link5 -->
    <joint name="joint4" type="revolute">
      <parent>link4</parent>
      <child>link5</child>
      <pose>0 0 -0.125 0 0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
    </joint>

    <!-- Joint 5: link5 - link6 -->
    <joint name="joint5" type="revolute">
      <parent>link5</parent>
      <child>link6</child>
      <pose>0 0 -0.125 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <plugin
      filename="gz-sim-joint-controller-system"
      name="gz::sim::systems::JointController">
      <joint_name>joint0</joint_name>
      <topic>/floating_manipulator/joint0</topic>
      <!--<initial_velocity>1.0</initial_velocity>-->
    </plugin>

    <plugin
      filename="gz-sim-joint-controller-system"
      name="gz::sim::systems::JointController">
      <joint_name>joint1</joint_name>
      <topic>/floating_manipulator/joint1</topic>
      <!--<initial_velocity>1.0</initial_velocity>-->
    </plugin>

    <plugin
      filename="gz-sim-joint-controller-system"
      name="gz::sim::systems::JointController">
      <joint_name>joint2</joint_name>
      <topic>/floating_manipulator/joint2</topic>
