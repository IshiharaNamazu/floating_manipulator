    <link name="link0">
      <!--  ******************pose -->
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>1700</mass>
        <inertia>
          <ixx>1434</ixx>
          <iyy>1434</iyy>
          <izz>1735</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>3.5 3.5 3.5</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>3.5 3.5 3.5</size>
          </box>
        </geometry>
      </visual>
    </link>

