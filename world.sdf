<?xml version="1.0"?>
<sdf version="1.9">
  <world name="no_grav_world">
    <plugin
      filename="ignition-gazebo-physics-system"
      name="ignition::gazebo::systems::Physics">
    </plugin>
    <plugin
      filename="ignition-gazebo-user-commands-system"
      name="ignition::gazebo::systems::UserCommands">
    </plugin>
    <plugin
      filename="ignition-gazebo-scene-broadcaster-system"
      name="ignition::gazebo::systems::SceneBroadcaster">
    </plugin>
    <plugin
      filename="ignition-gazebo-sensors-system"
      name="ignition::gazebo::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>

    <light name="front_directional" type="directional">
      <pose>0 0 2 0 0 0</pose> <!-- 少し上空に配置 -->
      <direction>-1 -1 -5</direction> <!-- 下向き -->
      <diffuse>1 1 1 1</diffuse>
      <specular>0.1 0.1 0.1 1</specular>
      <cast_shadows>true</cast_shadows>
    </light>


    <model name="ground_plane">
      <pose>0 0 1 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>1000 1000</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.8 0.8 0.8 1</specular>
          </material>
        </visual>
        <cast_shadows>false</cast_shadows>
      </link>
    </model>

    <gravity>0 0 0</gravity>
    <physics name="default_physics" type="ode">
      <max_step_size>0.001</max_step_size>
    </physics>

    <scene>
      <background>0 0 0 1</background>
      <ambient>0.05 0.05 0.05 1.0</ambient>
      <sky>
        <time>18.0</time> <!-- 読み込まれていない -->
      </sky>
    </scene>

    <include>
      <uri>file://floating_manipulator.xml</uri>
      <pose>0 0 11 0 -0.0990 0</pose>
    </include>

    <model name="box_object">
      <pose>-1.1990333683141545, -1.0196662233618718, 16.879911768849308, 1.537042091327021,
        -0.05597566138488763, -1.5169557508306544</pose>
      <static>false</static>
      <link name="box_link">
        <collision name="box_collision">
          <geometry>
            <box>
              <size>0.4 0.4 0.4</size>
            </box>
          </geometry>
        </collision>
        <visual name="box_visual">
          <geometry>
            <box>
              <size>0.4 0.4 0.4</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.7 0.7 0.7 1</diffuse>
            <specular>0.5 0.5 0.5 1</specular>
          </material>
        </visual>
      </link>
    </model>


  </world>
</sdf>